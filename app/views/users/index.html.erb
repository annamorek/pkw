<h1>Listing users</h1>
<table>
  <thead>
  <tr>
    <th>Login</th>
    <th>Role</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.login %></td>
        <td><%= user.role %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New User', new_user_path %>