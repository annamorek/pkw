<p id="notice"><%= notice %></p>

<h1>Ważne głosy oddane na poszczególne komitety wyborcze</h1>

<table>
  <thead>
    <tr>
      <th>Nazwa okręgu</th>
      <th>Komitet wyborczy</th>
      <th>Ilość głosów ważnych</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.nazwad %></td>
        <td><%= vote.nazwac %></td>
        <td><%= vote.vote %></td>
        <td><%= link_to 'Pokaż', vote if can? :update, Vote %></td>
        <td><%= link_to 'Edytuj', edit_vote_path(vote) if can? :update, Vote %></td>
      </tr>
    <% end %>
  </tbody>
</table>



